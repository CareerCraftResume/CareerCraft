{
  "name": "ai-powered-resume-builder",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "puppeteerSkipDownload": true,
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.0",
    "@mui/x-date-pickers": "^6.18.5",
    "@react-pdf/renderer": "^4.1.5",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.3.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.10",
    "express-async-handler": "^1.2.0",
    "firebase": "^11.1.0",
    "formik": "^2.4.0",
    "framer-motion": "^10.18.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "natural": "^8.0.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-to-pdf": "^1.0.1",
    "recharts": "^2.15.0",
    "stripe": "^17.5.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^9.1.0",
    "jest-canvas-mock": "^2.5.2",
    "msw": "^1.3.2",
    "puppeteer": "^23.11.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "nodemon backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "start:prod": "node backend/server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build && cd backend && npm install",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "create-indexes": "node backend/scripts/createIndexes.js",
    "update-indexes": "node backend/scripts/updateIndexes.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cacheDirectories": [
    "node_modules",
    "backend/node_modules"
  ]
}
